<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:skin id="quickstart"
      src        = "{require('ext/quickstart/quickstart').skin}"
      media-path = "/static/ext/quickstart/images/" />
    
    <a:window
        id = "quickStartDialog"
        title = ""
        center = "true"
        modal = "false"
        buttons = ""
        kbclose   = ""
        width     = "500"
        height    = "210"
        skinset   = "quickstart"
        skin ="quickstart"
        draggable = "false">
        <div id="quickStartHeader">
        <a:button skin="btn_icon_only" icon="close.png" onclick="require('ext/quickstart/quickstart').closeStart();"/>
        </div>

        <div id="quickStartTitle" class="qstitle">
            <p>Here are a few pointers to get you started!</p>
        </div>
        
        <div id="tourGuideText" class="tgtext">
            <p>For a detailed guided tour, <a href="#" class="tgLink" onclick="require('ext/quickstart/quickstart').shutdownQSStartGT();">click here!</a></p>
        </div>
        
         <div id="supportText" class="qsSupportText">
            <p>If you ever run into trouble, be sure to check out our support page on 
            <a href="http://c9.io/support.html" class="supportLink" target="_blank">c9.io/support</a>, 
            or talk to us on <a href="https://twitter.com/#!/C9Support" class="supportLink" target="_blank">Twitter</a>.</p>
        </div>
        
        <div class="qsCBContainer">
            <div class="checkboxDiv">
                 <a:checkbox id="quickStartCheckbox" 
                 skin    = "db-checkbox"
                    checked="true"
                  value   = "[{require('ext/settings/settings').model}::auto/help/@show]"
                  onafterchange="
                    if(this.checked)
                        trSaveAs.$ext.className = trSaveAs.$ext.className.replace(/showQS/, '');
                    else
                        apf.setStyleClass(trSaveAs.$ext, 'showQS');
                  ">Show this quick start on startup</a:checkbox>
            </div>
            <div class="buttonDiv">
            <a:button default="2" skin="btn-default-css3" class="btn-green tgPlayButton" 
            onclick="require('ext/quickstart/quickstart').closeStart();">Just the editor, please</a:button>              
            </div>
        </div>
    </a:window>
    
    <a:window
        id = "qsProjectBar"
        title = ""
        center = "false"
        modal = "false"
        buttons = ""
        kbclose   = "false"
        width     = "200"
        skinset   = "quickstart"
        skin ="qsImage"
        draggable = "false">
        <a:vbox class="qsProjectBar">
        </a:vbox>
    </a:window>
    
        <a:window
        id = "qsMenuBar"
        title = ""
        center = "false"
        modal = "false"
        buttons = ""
        kbclose   = "false"
        width     = "200"
        skinset   = "quickstart"
        skin ="qsImage"
        draggable = "false">
        <a:vbox class="qsMenuBar">
        </a:vbox>
    </a:window>
    
        <a:window
        id = "qsToolbar"
        title = ""
        center = "false"
        modal = "false"
        buttons = ""
        kbclose   = "false"
        width     = "200"
        skinset   = "quickstart"
        skin ="qsImage"
        draggable = "false">
        <a:vbox class="qsToolbar">
        </a:vbox>
    </a:window>
    
        <a:window
        id = "qsCLI"
        title = ""
        center = "false"
        modal = "false"
        buttons = ""
        kbclose   = "false"
        width     = "200"
        skinset   = "quickstart"
        skin ="qsImage"
        draggable = "false">
        <a:vbox class="qsCLI">
        </a:vbox>
    </a:window>
</a:application>